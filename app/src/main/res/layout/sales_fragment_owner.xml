<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="16dp"
    android:layout_marginRight="16dp"
    >


    <LinearLayout
        android:id="@+id/lin8"
        android:paddingTop="16dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="S.No"/>

        <TextView
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="19sp"
            android:hint="4CE0460D0G"/>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin1"
        android:layout_below="@+id/lin8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <GridView
            android:paddingTop="16dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/gridView1"
            android:numColumns="auto_fit"
            android:gravity="center"
            android:columnWidth="50dp"
            android:stretchMode="columnWidth"
            >

        </GridView>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin2"
        android:paddingTop="16dp"
        android:layout_below="@+id/lin1"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:text="Type"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="19sp"
            android:textSize="17sp"
            android:hint="Type"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin3"
        android:paddingTop="16dp"
        android:layout_below="@+id/lin2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:text="Description"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="19sp"
            android:textSize="17sp"
            android:hint="Desecription"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin4"
        android:layout_below="@+id/lin3"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:paddingTop="16dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:text="Price per Item"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="19sp"
            android:textSize="17sp"
            android:hint="$20"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/lin5"
        android:layout_below="@+id/lin4"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:paddingTop="16dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:text="Total price"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="19sp"
            android:textSize="17sp"
            android:hint="$120"/>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/lin6"
        android:layout_below="@+id/lin5"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:paddingTop="16dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:text="Duration"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="19sp"
            android:textSize="17sp"
            android:hint="3days"/>

    </LinearLayout>


    <LinearLayout
        android:id="@+id/lin7"
        android:layout_below="@+id/lin6"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:paddingTop="16dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:text="Payment status"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="19sp"
            android:textSize="17sp"
            android:hint="successful"/>


    </LinearLayout>


    <LinearLayout
        android:id="@+id/lin9"
        android:paddingTop="16dp"
        android:layout_below="@+id/lin7"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="17sp"
            android:text="Status"/>

        <TextView
            android:singleLine="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="19sp"
            android:textSize="17sp"
            android:hint="Complete/Pending/Rental Return"/>
    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lin9"
        android:layout_marginTop="10dp"
        android:gravity="center">

        <Button
            android:id="@+id/btnView"
            style="@style/DefaultButton"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:text="View"
            android:textSize="20sp"
            android:textStyle="bold" />


    </LinearLayout>


</RelativeLayout>
